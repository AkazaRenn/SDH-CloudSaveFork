  "author": "GedasFX",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/GedasFX/decky-cloud-save/issues"
  },
  "homepage": "https://github.com/GedasFX/decky-cloud-save#readme",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/webpack": "^5.28.5",
    "rollup": "^4.34.8",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "@decky/ui": "^4.9.1",
    "p-queue": "^8.1.0",
    "react-icons": "^5.5.0",
